{
    "name": "projectsaturnstudios/snj1-phaot",
    "description": "Takes a multicolor gcode file generated by Orcaslicer for the SnapmakerJ1, and appends gcode to enable early preheating of the next nozzle.",
    "keywords": ["framework", "laravel", "laravel zero", "console", "cli"],
    "homepage": "https://phoat.wethealchemists.com",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/projectsaturnstudios/SNJ1-PHAOT/issues",
        "source": "https://github.com/projectsaturnstudios/SNJ1-PHAOTo"
    },
    "authors": [
        {
            "name": "Angel Gonzalez",
            "email": "usftechacg@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2.0",
        "laravel-zero/framework": "^11.0.0",
        "parsecsv/php-parsecsv": "^1.3"
    },
    "require-dev": {
        "laravel/pint": "^1.15.2",
        "mockery/mockery": "^1.6.11",
        "pestphp/pest": "^2.34.7"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": ["j1"]
}
